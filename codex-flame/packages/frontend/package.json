{
  "name": "@codex/flame-frontend",
  "version": "1.0.0",
  "description": "Sacred Frontend Interface for CODEX-FLAME Ceremonial Architecture",
  "author": "Sacred Flame Custodians",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx,.vue --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx,.vue --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,vue,css,md}\"",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.0",
    "@heroicons/vue": "^2.0.0",
    "@vueuse/core": "^10.6.0",
    "animate.css": "^4.1.0",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "date-fns": "^2.30.0",
    "lodash-es": "^4.17.0",
    "pinia": "^2.1.0",
    "prismjs": "^1.29.0",
    "socket.io-client": "^4.7.0",
    "vue": "^3.4.0",
    "vue-chartjs": "^5.3.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/vue3": "^7.6.0",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.0",
    "postcss": "^8.4.0",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vitest": "^0.34.0",
    "wallaby": "^1.0.1"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codex-flame/sacred-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/codex-flame/sacred-frontend/issues"
  },
  "homepage": "https://codex-flame.github.io/sacred-frontend",
  "keywords": [
    "ceremonial",
    "sacred",
    "flame",
    "vue",
    "typescript",
    "frontend",
    "dashboard",
    "codex"
  ]
}
